<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.urttom.memory.xtpz.mapper.ThemeMapper">
    <select id="getThemeList" resultType="com.urttom.memory.xtpz.module.TXtpzTheme">
        select * from t_xtpz_theme where c_valid = '1' order by d_create_time desc
    </select>
</mapper>
