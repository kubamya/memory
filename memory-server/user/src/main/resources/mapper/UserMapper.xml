<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.urttom.memory.user.mapper.UserMapper">
    <select id="checkIsRegisted" resultType="com.urttom.memory.user.module.TSysUser">
        select c_id as cid from t_sys_user tsu where tsu.c_login_id = #{loginId}
    </select>
</mapper>
